{
  "name": "ACME Shell",
  "version": "1",
  "slug": "acme_sh",
  "description": "ACME Shell script for Let's Encrypt",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "once",
  "boot": "auto",
  "map": ["ssl:rw"],
  "options": {
    "domains": [null],
    "certfile": "fullchain.pem",
    "keyfile": "privkey.pem",
    "dns": {}
  },
  "schema": {
    "domains": ["str"],
    "certfile": "str",
    "keyfile": "str",
    "dns": {
      "provider": "list(dns_freedns)?",
      "username": "str?",
      "password": "str?"
    }
  }
}
