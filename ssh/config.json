{
  "name": "SSH server",
  "version": "0.8",
  "slug": "ssh",
  "description": "Connect to your server over SSH",
  "url": "https://home-assistant.io/addons/ssh/",
  "startup": "before",
  "boot": "auto",
  "ports": {
    "22/tcp": 22
   },
  "map": ["config:rw", "ssl:rw", "addons:rw", "share:rw", "backup:rw"],
  "options": {
    "authorized_keys": [null]
  },
  "schema": {
    "authorized_keys": ["str"]
  },
  "image": "homeassistant/{arch}-addon-ssh"
}
