{
  "name": "Duck DNS",
  "version": "0.6",
  "slug": "duckdns",
  "description": "Free Dynamic DNS (DynDNS or DDNS) service",
  "url": "https://home-assistant.io/addons/duckdns/",
  "startup": "services",
  "boot": "auto",
  "map": ["ssl:rw"],
  "options": {
    "accept_lets_encrypt_terms": false,
    "token": null,
    "domains": [null],
    "seconds": 300
  },
  "schema": {
    "accept_lets_encrypt_terms": "bool",
    "token": "str",
    "domains": ["str"],
    "seconds": "int"
  },
  "image": "homeassistant/{arch}-addon-duckdns"
}
