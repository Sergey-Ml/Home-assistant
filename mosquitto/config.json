{
  "name": "Mosquitto broker",
  "version": "0.3",
  "slug": "mosquitto",
  "description": "An Open Source MQTT broker",
  "startup": "before",
  "boot": "auto",
  "ports": {
    "1883/tcp": 1883,
    "8883/tcp": 8883
   },
  "map": ["ssl"],
  "options": {
    "plain": true,
    "ssl": false,
    "anonymous": true,
    "logins": [],
    "certfile": "fullchain.pem",
    "keyfile": "privkey.pem"
  },
  "schema": {
    "plain": "bool",
    "ssl": "bool",
    "anonymous": "bool",
    "logins": [
        {"username": "str", "password": "str"}
    ],
    "certfile": "str",
    "keyfile": "str"
  }
}
