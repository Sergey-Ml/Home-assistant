{
  "name": "Samba share",
  "version": "0.2",
  "slug": "samba",
  "description": "Expose HassIO folders with samba",
  "startup": "after",
  "boot": "auto",
  "ports": {
    "445/tcp": 445
   },
  "map": ["config", "ssl", "addons"],
  "options": {
    "workgroup": "WORKGROUP",
    "guest": true,
    "map_config": true,
    "map_addons": true,
    "map_ssl": false,
    "username": "",
    "password": ""
  },
  "schema": {
    "workgroup": "str",
    "guest": "bool",
    "map_config": "bool",
    "map_addons": "bool",
    "map_ssl": "bool",
    "username": "str",
    "password": "str"
  }
}
